 @using Microsoft.AspNetCore.Identity

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - asp_mvc_webpack</title>
</head>

<body>
    <div id="app" style="display: none;">
        <header-main>
            <header-login slot="partial-login" is-signedin="@(SignInManager.IsSignedIn(User))" user-name="@User.Identity.Name">
               
                @inject SignInManager<IdentityUser> SignInManager
                @inject UserManager<IdentityUser> UserManager

            </header-login>
        </header-main>

        <div class="container">
            <partial name="_CookieConsentPartial" />
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2019 - asp_mvc_webpack - <a asp-area="" asp-page="/Privacy">Privacy</a>
            </div>
        </footer>
    </div>
    <script src="~/js/main.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>